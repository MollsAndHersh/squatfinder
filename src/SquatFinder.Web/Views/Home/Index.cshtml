@{
    ViewData["Title"] = "DNS Twister Monitor";
}

<style>
    .navbar-brand h1 {
        font-size: 1em;
        margin-top: auto;
    }

    .domain-group {
        padding-bottom: 20px;
    }

    .thumbnail-container {
        width: 200px;
        margin: 0px auto;
    }

    .thumbnail-fixed {
        position: fixed;
        width: 200px;
    }

    .header-top {
        display: none;
        background-color: white;
        padding-top: 10px;
        padding-bottom: 10px;
        box-shadow: 0px 2px lightgray;
    }

    .monitor-title {
        font-size: 0.9em;
        text-transform: uppercase;
        font-weight: bold;
        display: block;
    }

    .monitor-sub-title {
        color: #5cb85c;
    }

    .monitor-value {
        font-size: 4em;
        display: block;
        padding-top: 40px;
        font-weight: lighter;
        color: blue;
    }

</style>

<nav class="navbar navbar-fixed-top">
    <div class="navbar-inverse" style="border-radius: 0px">
        <div class="container" role="navigation">
            <div class="row">
                <div class="navbar-header">
                    <a class="navbar-brand" href="">
                        <h1>DNS Twister Monitor</h1>
                    </a>
                </div>
            </div>
        </div>
    </div>
    <div class="header-top">
        <div class="container">
            <div class="row">
                <div class="col-sm-6 col-md-9">
                    <div class="input-group input-group-lg">
                        <input type="text" class="form-control" placeholder="Domain Name" />
                        <span class="input-group-btn">
                    <button type="button" class="btn btn-success btn-lg">Analyse</button>
                </span>
                    </div>
                </div>
                <div class="col-sm-6 col-md-3">
                    <div class="thumbnail-container">
                        <div class="thumbnail thumbnail-fixed">
                            <img alt="100%x180" class="img-rounded" data-src="holder.js/100%x180" style="height: 180px; width: 100%; display: block;"
                                src="data:image/svg+xml;base64,"
                                data-holder-rendered="true">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</nav>

<div class="container theme-showcase" role="main">

    <div class="row">
        <div class="col-sm-6 col-md-9">
            <h2>Domain Name</h2>
            <form class="domain-group input-group input-group-lg" asp-controller="Home" asp-action="Index" method="post">
                <input type="text" class="form-control" placeholder="Domain Name" id="domainName" name="domainName" />
                <span class="input-group-btn">
                    <button type="submit" class="btn btn-success btn-lg">Analyse</button>
                </span>
            </form>
        </div>
        <div class="col-sm-6 col-md-3">
            <div class="thumbnail-container" id="domain-thumbnail-container">
                <div class="thumbnail">
                    <img alt="100%x180" class="img-rounded" data-src="holder.js/100%x180" style="height: 180px; width: 100%; display: block;"
                        src="data:image/svg+xml;base64,"
                        data-holder-rendered="true">
                </div>
            </div>
        </div>
    </div>

    @if (Model != null)
    {
        <div class="page-header">
            <h2>Monitors</h2>
        </div>

        <div class="row">
            <div class="col-sm-6 col-md-3">
                <div class="well">
                    <span class="monitor-title">Total Domain</span>
                    <span class="monitor-title">Combinations</span>
                    <span class="monitor-value">197</span>
                </div>
            </div>
            @foreach (var type in Model.DomainFuzzerTypesList)
            {
                @if (type.FuzzerType != "Original*")
                {
                    <div class="col-sm-6 col-md-3">
                        <div class="well">
                            <span class="monitor-title">Mode</span>
                            <span class="monitor-title monitor-sub-title">@type.FuzzerType</span>
                            <span class="monitor-value">@type.Count @Html.Raw($"[{type.Percentage}%]")</span>
                        </div>
                    </div>
                }
            }
        </div>

		<div class="page-header">
			<h2>Screenshots</h2>
		</div>

		<div class="row">
			<div class="col-sm-6 col-md-3">
