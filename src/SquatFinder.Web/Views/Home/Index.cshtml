@section scripts {
    <script>
        $(document).ready(function () {
            var wrap = $(document);

            wrap.on("scroll", function (e) {

                var scrollingElement = this.scrollingElement;

                if (scrollingElement.scrollTop > 74) {
                    $("#domain-thumbnail-container").parent(".row").hide();
                    $(".header-top").show();
                } else {
                    $("#domain-thumbnail-container").parent(".row").show();
                    $(".header-top").hide();
                }

            });
        });
    </script>
}
